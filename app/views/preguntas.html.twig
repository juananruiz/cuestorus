{% extends 'layout.html.twig' %}
{% set active = 'preguntas' %}
{% set title = 'Lista de Preguntas' %}

{% block content %}
    <div class="container">
        <h2>Lista de preguntas</h2>
        <ul>
            {% for pregunta in preguntas %}
                <li>
                    {{ pregunta.enunciado|e }}, {{ pregunta.id|e }}, {{ pregunta.orden|e }}
                </li>
            {% endfor %}
        </ul>
    </div>
{% endblock %}
